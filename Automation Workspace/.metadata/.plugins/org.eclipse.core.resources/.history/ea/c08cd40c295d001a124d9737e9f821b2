package vaa;

public class method {
static void myMethod(String fname, int age)
{
	System.out.println(fname + " is" + age);
}
	public static void main(String[] args) {
		myMethod("RDJ", 41);
		myMethod("taylor", 31);
		myMethod("penny", 45);
		myMethod("ross", 41);
	myMethod("joey", 43);
	myMethod("rachel", 44);
	}

}
