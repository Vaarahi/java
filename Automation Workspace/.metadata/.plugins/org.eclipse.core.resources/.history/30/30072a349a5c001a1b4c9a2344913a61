package vaa;

public class Arrays {

	public static void main(String[] args) {
      String[] languages = {"java","python","ruby","dotnet"};
      System.out.println(languages.length);
      int[][] num = {{1,2,3},{4,3,2}};
      for(int i=0;i<num[i].length;i++)
      {
    	  for(int j=0;j<num[j].length;j++) {
    		  System.out.println(num[i][j]);
    	  }
      }

}
}